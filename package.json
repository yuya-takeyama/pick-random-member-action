{
  "name": "@yuya-takeyama/pick-random-member-action",
  "version": "0.1.1",
  "private": true,
  "description": "Pick a random member from a team in GitHub Issues/Pull Requests",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "keywords": [
    "random",
    "member",
    "comment"
  ],
  "author": "Yuya Takeyama",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@typescript-eslint/parser": "^4.24.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.21.0",
    "eslint-plugin-github": "^4.1.2",
    "eslint-plugin-jest": "^24.1.8",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.2.3"
  }
}
